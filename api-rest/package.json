{
    "name": "zalarify_api",
    "version": "0.3.7",
    "description": "Project for Zalarify Main API backend",
    "main": "dist",
    "scripts": {
        "clean": "rm -rf dist && mkdir dist",
        "dev": "nodemon --exec babel-node src",
        "build": "babel src -s -D -d dist",
        "start": "node dist",
        "prestart": "npm run -s clean && npm run -s build",
        "test": "jest",
        "test:mocha": "mocha -t 15000 --require @babel/register --require @babel/polyfill \"tests/**/*.js\"",
        "testu": "mocha -t 15000 --require @babel/register --require @babel/polyfill",
        "heroku-postbuild": "npm run postbuild"
    },
    "author": "guillesalazar@gmail.com",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.18.0",
        "bignumber.js": "8.0.1",
        "body-parser": "^1.13.3",
        "compression": "^1.5.2",
        "cors": "^2.7.1",
        "dotenv": "^6.2.0",
        "ethereumjs-util": "5.1.5",
        "ethers": "^4.0.23",
        "express": "^4.16.4",
        "knex": "^0.17.3",
        "lodash": "^4.17.11",
        "morgan": "^1.8.0",
        "objection": "^1.6.9",
        "pg": "^7.11.0",
        "resource-router-middleware": "^0.7.0",
        "web3": "^1.0.0-beta.55"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.1",
        "@babel/register": "^7.0.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^23.6.0",
        "chai": "^4.2.0",
        "eslint": "^5.13.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jest": "^22.4.1",
        "jest": "^23.6.0",
        "leche": "^2.2.3",
        "mocha": "^6.1.3",
        "nock": "^10.0.6",
        "nodemon": "^1.9.2",
        "prettier": "^1.16.4",
        "regenerator-runtime": "^0.12.1"
    },
    "engines": {
        "node": "10.15.x"
    }
}
